  variable Table[VAR_SIZE];

  void TableInit() {
  Table[0].key = NULL;
  Table[0].value = 0;
  }

  int look(const char *key, int top) {
  size_t i = top;
  while (Table[i].key != NULL) {
  if (strcmp(key, Table[i].key) == 0) {
  return Table[i].value;
  }
  else {
  i--;
  }
  }
  return 0;
  }